	<style>
		.main-menu > li > a {
			font-family: Poppins-Medium;
			font-size: 16px;
			font-weight: bold;
			color: #333;
			padding: 5px 0px;
			transition: all 0.4s;
			-webkit-transition: all 0.4s;
			-o-transition: all 0.4s;
			-moz-transition: all 0.4s;
		}

		.main-menu {
			margin-top: 3%;
		}
	</style>

</head>

<body>
	<header>
		<!-- Header desktop -->
		<div class="container-menu-desktop">
			<!-- Topbar -->
			<div class="top-bar">
				<div class="content-topbar flex-sb-m h-full container">
					<div class="left-top-bar ">
						Enjoy your online shopping with our store
					</div>

					<div class="right-top-bar flex-w h-full custom-text" style="font-family: 'Acme', sans-serif;">

						<a asp-action="About2" asp-controller="UserHome" class="flex-c-m trans-04 p-lr-25">
							About Us
						</a>

						<a asp-action="Contact2" asp-controller="UserHome" class="flex-c-m trans-04 p-lr-25">Contact</a>

						<a asp-action="FAQ2" asp-controller="UserHome" class="flex-c-m trans-04 p-lr-25">
							Help & FAQs
						</a>

						<a asp-action="Privacy2" asp-controller="UserHome" class="flex-c-m trans-04 p-lr-25">Policy</a>
					</div>
				</div>
			</div>

			<div class="wrap-menu-desktop">
				<nav class="limiter-menu-desktop container">

					<!-- Logo desktop -->
					<a asp-action="Index" asp-controller="UserHome" class="logo">
						<img src="~/hometem/logo.png" alt="IMG-LOGO" width="150" />
					</a>

					<!-- Menu desktop -->
					<div class="menu-desktop">
						<ul class="main-menu">
							<li class="active-menu" style="font-family: 'Chivo', sans-serif;">


								<a asp-action="Index" asp-controller="UserHome"> Home </a>

							</li>

							<li>
								<a asp-action="Categories2" asp-controller="UserHome">Categories</a>
							</li>

							<li class="label1" data-label1="hot">
								<a asp-action="Products2" asp-controller="UserHome">Discover</a>
							</li>



							<li>
								<a asp-action="TestmonialsNavBar" asp-controller="UserHome">Testmonials</a>
							</li>

						</ul>
					</div>






				</nav>
			</div>
		</div>



		<!-- Header Mobile -->
		<div class="wrap-header-mobile">
			<!-- Logo moblie -->
			<div class="logo-mobile">
				<a asp-action="Index" asp-controller="UserHome" class="logo">
					<img src="~/hometem/logo.png" alt="IMG-LOGO" width="150" />
				</a>
			</div>

			<!-- Icon header -->
			<div class="wrap-icon-header flex-w flex-r-m m-r-15">
				<a asp-action="MyCart" asp-controller="Carts" class="dis-block icon-header-item cl2 hov-cl1 trans-04 p-l-22 p-r-11 ">
					<i class="fa-sharp fa-solid fa-cart-shopping" style="color: #000; font-size: 30px"></i>
				</a>


				<a asp-action="Wishlist" asp-controller="Wishlists" class="dis-block icon-header-item cl2 hov-cl1 trans-04 p-l-22 p-r-11 ">
					<i class="zmdi zmdi-favorite-outline" style="color:#000; font-size: 35px"></i>
				</a>
				<div class="icon-header-item cl2 hov-cl1 trans-04 p-r-11 js-show-modal-search">
					<i class="zmdi zmdi-search" style="font-size:30px;"></i>
				</div>
				

			</div>

			<!-- Button show menu -->
			<div class="btn-show-menu-mobile hamburger hamburger--squeeze">
				<span class="hamburger-box">
					<span class="hamburger-inner"></span>
				</span>
			</div>
		</div>


		<!-- Menu Mobile -->
		<div class="menu-mobile">
			<ul class="topbar-mobile">
				<li>
					<div class="left-top-bar">
					</div>
				</li>

				<li>
					<div class="left-top-bar ">
					</div>




					<div class="right-top-bar flex-w h-full custom-text" style="font-family: 'Acme', sans-serif;">
						<a asp-action="About2" asp-controller="UserHome" class="flex-c-m trans-04 p-lr-25">
							About Us
						</a>


						<a asp-action="Contact2" asp-controller="UserHome" class="flex-c-m trans-04 p-lr-25">Contact</a>
						<a asp-action="FAQ2" asp-controller="UserHome" class="flex-c-m trans-04 p-lr-25">Help & FAQs</a>
						<a asp-action="Privacy2" asp-controller="UserHome" class="flex-c-m trans-04 p-lr-25">Policy</a>



					</div>
				</li>
			</ul>

			<ul class="main-menu-m">
				<li class="active-menu" style="font-family: 'Chivo', sans-serif;">


					<a asp-action="Index" asp-controller="UserHome"> Home </a>

				</li>

				<li>
					<a asp-action="Categories2" asp-controller="UserHome">Categories</a>
				</li>

				<li>
					<a asp-action="Products2" asp-controller="UserHome">Discover</a>
				</li>




				<li>
					<a asp-action="TestmonialsNavBar" asp-controller="UserHome">Testimonials</a>
				</li>

			</ul>
		</div>


		<!-- Modal Search -->
		<div class="modal-search-header flex-c-m trans-04 js-hide-modal-search">
			<div class="container-search-header">
				<button class="flex-c-m btn-hide-modal-search trans-04 js-hide-modal-search">
					<img src="~/HomeTem/images/icons/icon-close2.png" alt="CLOSE" />
				</button>

				<form class="wrap-search-header flex-w p-l-15">
					<button class="flex-c-m trans-04">
						<i class="zmdi zmdi-search"></i>
					</button>
					<input class="plh3" type="text" name="search" placeholder="Search...">
				</form>
			</div>
		</div>

	</header>
    <div>
        @RenderBody()
    </div>


	
	<!-- Footer -->
	<footer class="bg3  p-b-32">
		<div class="container">


			<div class="p-t-40">


				<p class="stext-107 cl6 txt-center">
					Contact us:  HeAndSheSupport@gmail.com
				</p>
				<p class="stext-107 cl6 txt-center">
					Copyright &copy;<script>document.write(new Date().getFullYear());</script> | All rights reserved To Esraa Alshaikh 🤍
				</p>
			</div>
		</div>
	</footer>


